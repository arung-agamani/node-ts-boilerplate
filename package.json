{
  "name": "node-ts-boilerplate",
  "version": "0.0.1",
  "description": "Simple boilerplate for Node.js with TypeScript projects",
  "main": "index.ts",
  "author": "Arung Agamani",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@types/node": "^14.14.8",
    "eslint": "^7.13.0",
    "husky": "^5.0.1",
    "lint-staged": "^10.5.1",
    "pinst": "^2.1.1",
    "prettier": "2.1.2",
    "typescript": "^4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
